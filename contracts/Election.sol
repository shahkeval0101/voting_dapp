pragma solidity 0.4.23;

/**
 * The Election contract does this and that...
 */
contract Election {

    /*//store the candidate
    //read the candidate
    string public candidate;//since its defined as public it will be function and not variable
    //constructor
    constructor() public {
        candidate="Candidate 1";
    }*/
    // Model a Candidate
    struct Candidate {
        uint id;
        string name;
        uint voteCount;
    }

    // Store accounts that have voted
    mapping(address => bool) public voters;//take a account address as key of map and
    // Store Candidates
    // Fetch Candidate
    mapping(uint => Candidate) public candidates;//since its public it is function and not variable
        //with mapping we can instatiate the struct
        //its key is number and value struct Candidate
        //there is no way to determine the size of the mapping and to iterate over it so we keep the candidate count
        // Store Candidates Count
        //this example will store two candidates
    uint public candidatesCount;//camel cache

    // voted event
    event votedEvent (
        uint indexed _candidateId
    );

    constructor () public {//they should called as whenever our smart contract gets deployed it should be done that time
        addCandidate("Candidate 1");
        addCandidate("Candidate 2");
    }

    function addCandidate (string _name) private {//should be private as it should be handle by our smartcontract
        candidatesCount ++;
        candidates[candidatesCount] = Candidate(candidatesCount, _name, 0);//at next mapping insert struct with number,name,initial no of the vote
    }

    function vote (uint _candidateId) public {
        // require that they haven't voted before
        require(!voters[msg.sender]);//initially should be false and not of it true and thus then only should go further

        // require a valid candidate
        require(_candidateId > 0 && _candidateId <= candidatesCount);

        // record that voter has voted
        voters[msg.sender] = true;

        // update candidate vote Count
        candidates[_candidateId].voteCount ++;//whoever calls it on that id vote count should increamented by one

        // trigger voted event
        emit votedEvent(_candidateId);
    }
}

